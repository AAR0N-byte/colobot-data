#: ../help/help.E.txt:8
#, no-wrap
msgid "Note"
msgstr ""

#. type: Plain text
#: ../help/help.E.txt:30
#, no-wrap
msgid ""
"\\tab;>>> CAUTION! Enemies are close!\n"
"\\tab;>>> Uploading of the SpecializedAntiAlienDefenseProgram completed.\n"
"\\tab;>>> It can be run from the 1. position on the list.\n"
"\\tab;>>> Thanks for choosing Terranova Communications and have a nice day!"
msgstr ""

