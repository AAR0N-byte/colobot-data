#. type: \b; header
#: ../help/soluce.E.txt:1
#, no-wrap
msgid "Walkthrough"
msgstr "Schritt für Schritt"

#. type: Bullet: '1)'
#: ../help/soluce.E.txt:2
#, no-wrap
msgid "Thump the deck of the <a object|base>spaceship</a>. You don't need to move the <a object|bottump>thumper</a> anywhere."
msgstr "Stampfen Sie auf das Deck des <a object|base>Raumschiffes</a>. Sie brauchen den <a object|bottump>Stampfer</a> dazu nicht zu bewegen."

#. type: Bullet: '2)'
#: ../help/soluce.E.txt:3
#, no-wrap
msgid "Locate the <a object|botoj>orga shooter</a> approximately 70 meters to the east."
msgstr "Finden Sie den <a object|botoj>Orgashooter</a> ungefähr 70 Meter weit östlich."

#. type: Bullet: '3)'
#: ../help/soluce.E.txt:4
#, no-wrap
msgid "Terminate the <a object|ant>ants</a> while they are still on their backs."
msgstr "Zerstören Sie die <a object|ant>Ameisen</a> solange sie noch auf dem Rücken liegen."

#. type: Bullet: '4)'
#: ../help/soluce.E.txt:5
#, no-wrap
msgid "Take off."
msgstr "Heben Sie ab."

